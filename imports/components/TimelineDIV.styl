.TimelineDIV

  .svg
    background-color black
    overflow hidden

  .wrapper
    position relative

    > div
      position absolute

  .friend
    width 2px
    &:hover
      background-color rgba(200,200,200,0.1)

  .period
    position absolute
    width 2px
    background-color #42b72a
    // padding-bottom 5px

  .period::after
    width 100%
    height 10px
    bottom 0
    background-color red
    position absolute

