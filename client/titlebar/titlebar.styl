#titlebar

  position fixed
  z-index 100
  width 100%
  height 42px
  border-bottom 1px solid #29487d

  line-height @height
  font-size 13px
  // font-weight 500

  background-color #3B5998
  color #CCC

  .titleContainer
    max-width 960px
    margin 0 auto

  input
    max-width: 448px;
    min-width: 344px;
    border: 1px solid #3b5998;
    background-color: #fff;
    border-radius: 3px;
    box-sizing: border-box;
    height: 26px;
    overflow: hidden;
    padding-left: 5px;
    padding-right: 48px;
    position: relative;
    margin-left 8px
    // top: 8px;

